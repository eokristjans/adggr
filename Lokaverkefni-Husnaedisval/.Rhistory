findMaxSumLessThanS <- function(arr, n, S){
curr_sum = arr[1]
max_sum = 0
start = 1
for(i in 1:n) {
if(curr_sum <= S){
max_sum = max(max_sum, curr_sum)
}
while(TRUE) {
if(curr_sum+arr[i] > S & start < i){
curr_sum = curr_sum - arr[start]
print(curr_sum+arr[i] > S)
print(start < i)
start = start+1
}
}
curr_sum = curr_sum + arr[i]
}
if(curr_sum <= S){
max_sum = max(max_sum, curr_sum)
}
return(max_sum)
}
